#include <iostream>
using namespace std;

int main() {
	// your code goes here
	int t;
	cin>>t;
	while(t--){
	    int n;
	    cin>>n;
	    string s;
	    int num=0;
	    cin>>s;
	    
	    if(s=="1" or s=="10")cout <<"NO\n";
	    else{
	        for(int i=0;i<n;i++){
	            if(s[i]=='1')num++;
	        }
	        if(num<=3)
	        cout<<"YES\n";
	        else
	        cout<<"NO\n";
	    }
	}
	return 0;
}
